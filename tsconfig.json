{
  "files": [],
  "extends": [
    "@vue/tsconfig/tsconfig.dom.json",
    "@tsconfig/node18/tsconfig.json"
  ],
  "include": [
    "env.d.ts",
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/**/*.d.ts",
    "src/**/*.json",
    "src/**/*.service.ts",
    "src/**/*.vue",
    "vite.config.*",
    "vitest.config.*",
    "cypress.config.*",
    "nightwatch.conf.*",
    "playwright.config.*"
  ],
  "exclude": [
    "src/**/__tests__/*"
  ],
  "compilerOptions": {
    "composite": true,
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "types": [
      "node"
    ],
    "baseUrl": ".",
    "paths": {
      "__/*": [
        "./src/*"
      ]
    },
    "lib": [
      "dom"
    ]
  }
}